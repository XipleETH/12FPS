{
  "private": true,
  "name": "docefps",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "build:client": "vite build --config src/client/vite.config.ts",
    "build:server": "vite build --config src/server/vite.config.ts",
    "build:all": "npm run build:client && npm run build:server",
    "build": "npm run build:all",
    "check": "npm run type-check && npm run lint:fix && npm run prettier",
    "deploy": "npm run build && devvit upload",
    "dev:client": "vite build --config src/client/vite.config.ts --watch",
    "dev:server": "vite build --config src/server/vite.config.ts --watch",
    "dev:vite": "vite --config src/client/vite.config.ts --port 7474",
    "dev:playtest": "concurrently -k -p '[{name}]' -n CLIENT,SERVER,DEVVIT -c blue,green,magenta \"npm run dev:client\" \"npm run dev:server\" \"npm run dev:devvit\"",
    "dev:local": "concurrently -k -p '[{name}]' -n CLIENT,SERVER -c blue,green \"npm run dev:client\" \"npm run dev:server\"",
    "dev": "npm run dev:local",
    "dev:devvit": "devvit playtest r/12FPS",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "login": "devvit login",
    "launch": "npm run build && npm run deploy && devvit publish",
    "prettier": "prettier-package-json --write ./package.json && prettier --write .",
    "type-check": "tsc --build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.886.0",
    "@devvit/web": "0.12.0",
    "clsx": "2.1.1",
    "devvit": "0.12.0",
    "express": "5.1.0",
    "lucide-react": "^0.344.0",
  "react": "18.3.1",
  "react-dom": "18.3.1",
    "tailwind-merge": "3.3.0"
  },
  "devDependencies": {
    "@eslint/js": "9.23.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/vite": "4.1.6",
    "@types/express": "5.0.1",
    "@types/react": "19.1.4",
    "@types/react-dom": "19.1.5",
    "@vitejs/plugin-react": "4.4.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "9.1.2",
    "dotenv-cli": "8.0.0",
    "eslint": "9.23.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "15.15.0",
    "prettier": "3.5.3",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tailwindcss": "4.1.6",
    "typescript": "5.8.2",
    "typescript-eslint": "8.29.0",
    "vite": "6.2.4",
    "vitest": "3.1.1"
  }
}